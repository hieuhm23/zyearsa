{
  "expo": {
    "name": "ZyeaRSA",
    "slug": "toke",
    "version": "1.0.1",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zyea.rsa",
      "infoPlist": {
        "NSCameraUsageDescription": "Ứng dụng cần truy cập Camera để quét mã vạch thuốc.",
        "NSPhotoLibraryUsageDescription": "Ứng dụng cần truy cập Thư viện ảnh để chọn ảnh sản phẩm."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Ứng dụng cần truy cập Camera để quét mã vạch thuốc."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Ứng dụng cần truy cập Thư viện ảnh để chọn ảnh sản phẩm.",
          "cameraPermission": "Ứng dụng cần truy cập Camera để chụp ảnh sản phẩm."
        }
      ],
      "expo-updates",
      "expo-secure-store"
    ],
    "updates": {
      "url": "https://u.expo.dev/22440a9f-50a7-4aca-9af3-6a3a1c453321",
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "checkOnLaunch": "ALWAYS",
      "requestHeaders": {
        "expo-channel-name": "production"
      }
    },
    "extra": {
      "eas": {
        "projectId": "22440a9f-50a7-4aca-9af3-6a3a1c453321"
      }
    },
    "owner": "hieukka"
  }
}